package com.strings;

public class test5 {
public static void main(String[] args) {
	String str ="Java";
	//concat
	str.concat("world");
	
	//length
	System.out.println(str.length());
	
	//charAt(i)
	System.out.println(str.charAt(3));
	
	//indexOf()
	System.out.println(str.indexOf('a'));
	
	//lastIndexOf()
	System.out.println(str.lastIndexOf('a'));
}
}
